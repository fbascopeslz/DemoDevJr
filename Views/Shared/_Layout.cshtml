        <section>
            @{
                int i = 0;
            }

            @{
                IEnumerable<DemoDevJr.Models.AlumnoInscripcion> datos = ViewBag.datos;
            }

            <div class="container">
                <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        @foreach (var ele in datos)
                        {
                            string clase = "";
                            if (i == 0)
                            {
                                clase = "active";
                            }
                            else
                            {
                                clase = "";
                            }
                            <li data-target="#carouselExampleIndicators" data-slide-to="@i" class="@clase"></li>

                            i++;
                        }
                    </ol>
                    <div class="carousel-inner">
                        @{
                            i = 0;
                        }
                        @foreach (var item in datos)
                        {
                            string clase = "";
                            if (i == 0)
                            {
                                clase = "carousel-item active";
                            }
                            else
                            {
                                clase = "carousel-item";
                            }
                            <div class="@clase">
                                <div class="card text-white bg-dark mb-3">
                                    <div class="card-header">
                                        <h5 class="card-title text-bold">@Html.DisplayFor(modelItem => @item.nombres) @Html.DisplayFor(modelItem => @item.apellidoPaterno) @Html.DisplayFor(modelItem => @item.apellidoMaterno)</h5>
                                    </div>
                                    <div class="card-body">
                                        <div class="row d-flex justify-content-start align-items-center">
                                            <div class="col-6">
                                                <img class="d-block w-100" src="@item.imagen" alt="@item.id">
                                            </div>
                                            <div class="col-6">
                                                <p class="card-text"><b>Nombres: </b> @item.nombres</p>
                                                <p class="card-text"><b>Apellido Paterno: </b> @item.apellidoPaterno</p>
                                                <p class="card-text"><b>Apellido Materno: </b> @item.apellidoMaterno</p>
                                                <p class="card-text"><b>Lugar Nacimiento: </b> @item.lugarNacimiento</p>
                                                <p class="card-text"><b>Fecha Nacimiento: </b> @item.fechaNacimiento</p>
                                                <p class="card-text"><b>CI: </b> @item.ci</p>
                                                <p class="card-text"><b>Direccion: </b> @item.direccion</p>
                                                <p class="card-text"><b>Zona: </b> @item.zona</p>
                                                <p class="card-text"><b>Telefono: </b> @item.telefono</p>
                                                <p class="card-text"><b>RUDE: </b> @item.rude</p>
                                                <p class="card-text"><b>Curso: </b> @item.curso</p>
                                            </div>
                                        </div>
                                        <p class="card-text m-3"><small class="text-muted"><b>Fecha de Inscripcion: </b>@item.fechaInscripcion</small></p>
                                    </div>
                                </div>
                            </div>
                            i++;
                        }
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
